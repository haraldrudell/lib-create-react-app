{
  "name": "lib-create-react-app",
  "version": "0.0.2",
  "description": "Share React components from create react app by Harald Rudell",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "ISC",
  "scripts": {
    "build": "npm-run-all --serial 'build:*' ",
    "build:binPrepLib": "rollup --config src/rollup.config.js",
    "build:eslint": "eslint src/rollup.config.js src/builder/rollup.config.js src/rollup/*.js",
    "build:cleanup": "rimraf tmp lib",
    "clean": "rimraf bin",
    "reinstall": "npm-run-all --serial reinstall:*",
    "reinstall:clean": "rimraf node_modules",
    "reinstall:install": "yarn",
    "installtest": "jest --testRegex 'src/.*itest.*'",
    "test": "jest",
    "pub": "npm-run-all --serial test 'pub:*' ",
    "pub:build": "bin/preplib build",
    "pub:yarn": "echo yarn publish"
  },
  "main": "lib/libCreateReactApp",
  "module": "lib/libCreateReactApp",
  "bin": {
    "preplib": "bin/preplib"
  },
  "keywords": [
    "Create React App",
    "React",
    "Component",
    "Library",
    "lib",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haraldrudell/lib-create-react-app"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "useBuiltIns": "entry"
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/tmp/"
    ]
  },
  "dependencies": {
    "allspawn": "^0.2.7",
    "@babel/core": "7",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "fs-extra": "^7.0.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "files": [
    "bin",
    "src",
    "README.md"
  ]
}